{
  "name": "todo-app",
  "version": "1.0.0",
  "description": "todo app for wrestlers",
  "main": "server.js",
  "scripts": {
    "test": "  nyc --reporter=html --reporter=text  mocha --require babel-polyfill --require @babel/register --recursive --exit  src/test/*.js --timeout 90000",
    "start": "node ./build/src/app.js ",
    "build": " npm run db:migrate && babel src/. --out-dir build/src"
,   "dev": "nodemon  --exec babel-node src/app.js",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "coverage": "  nyc report --reporter=text-lcov | coveralls",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo:all": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "db:migrate:undo": "./node_modules/.bin/sequelize db:migrate:undo"
  },
  "author": "Robert",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@hapi/joi": "^15.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "core-js": "^3.8.1",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lcov-total": "^1.0.5",
    "mocha": "^8.2.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "regenerator-runtime": "^0.13.7",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.0",
    "swagger-ui-express": "^4.1.6"
  },
  "engines": {
    "node": "14.15.0",
    "npm": "6.14.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "babel-plugin-istanbul": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.6"
  }
}
