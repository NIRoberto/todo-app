{
  "env": { "node": true, "mocha": true, "es6": true },
  "extends": ["airbnb-base"],
  "parserOptions": {  "props": true , "code": 250,"ignoreComments": true ,"tabWidth": 4 ,"no-var": 0},
  "rules": {
     "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
     "import/no-extraneous-dependencies": ["error", {"devDependencies": false}],
      "import/prefer-default-export": 0,
      "no-console": 0,
      "no-shadow": 0 ,
      "max-len": 0,
      "no-restricted-syntax": 0,
      "eslint block-scoped-var": 0,
      "one-var-declaration-per-line": 0,
      "new-cap": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
       "curly": ["error", "multi-line"],
      "comma-dangle": 0,
      "func-names":0,
      "vars-on-top":0,
      "no-undef":0,
      "no-return-assign":0,
      "prefer-spread":0,
      "no-use-before-defin":0,
      " no-sequences ":0,
      "no-unused-expressio":0,
      "no-sequences ":0,
      " no-nested-ternary":0,
      "prefer-rest-params ":0,"no-mixed-operators":0

      


      

      
     
  }
}